// api/webhook.js
export default function handler(req, res) {
  if (req.method === 'POST') {
    console.log('Received Webhook:', req.body);  
  } else {
    res.setHeader('Allow', ['POST']);
    res.status(405).end(`Method ${req.method} Not Allowed`);
  }
}
